<zk>
	<window sclass="panel-info panel-sm" 
		apply="${vistaNotificationProcessingController}" mode="overlapped"
		position="right,bottom" mold="bootstrap" visible="false">
		<caption id="caption" />
		<div align="center">
			<button id="btnNext" zclass="btn"
				sclass="btn-success btn-xs"
				label="${labels.vistanotification.processing.button.next.label}" />
			<button id="btnStop" zclass="btn" sclass="btn-danger btn-xs"
				label="${labels.vistanotification.processing.button.stop.label}" />
		</div>
	</window>
</zk>