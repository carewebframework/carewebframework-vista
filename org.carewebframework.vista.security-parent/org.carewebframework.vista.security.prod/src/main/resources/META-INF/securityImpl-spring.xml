<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://www.springframework.org/schema/beans
               http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">

	<beans profile="root">

		<bean id="securityService" class="org.carewebframework.vista.security.prod.SecurityServiceImpl">
			<property name="logoutTarget" value="${org.carewebframework.security.logout.url:}" />
		</bean>

		<bean id="securityAuthenticationProvider" class="org.carewebframework.vista.security.base.BaseAuthenticationProvider">
			<property name="brokerSession" ref="brokerSession" />
		</bean>
	
	</beans>
</beans>